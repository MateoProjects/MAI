    <sentence id="DDI-DrugBank.d30.s1" text="Rifampin, an inducer of drug metabolism, decreased the concentrations of losartan and its active metabolite.">
        <entity id="DDI-DrugBank.d30.s1.e0" charOffset="0-7" type="drug" text="Rifampin"/>
