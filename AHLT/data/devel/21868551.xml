        <pair id="DDI-MedLine.d164.s4.p0" e1="DDI-MedLine.d164.s4.e0"
            e2="DDI-MedLine.d164.s4.e1" ddi="true" type="effect"/>
    </sentence>
    <sentence id="DDI-MedLine.d164.s5" text="Lapatinib may have the potential to convert Herceptin-refractory to Herceptin-sensitive tumors in HER2-positive breast cancer by up-regulation of the cell surface expression of HER2.">
