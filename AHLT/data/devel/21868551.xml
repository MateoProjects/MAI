        <pair id="DDI-MedLine.d164.s0.p0" e1="DDI-MedLine.d164.s0.e0"
            e2="DDI-MedLine.d164.s0.e1" ddi="true" type="effect"/>
    </sentence>
    <sentence id="DDI-MedLine.d164.s1" text="Although it was previously reported that lapatinib combined with Herceptin improved the progression-free survival rate compared with lapatinib alone for patients with Herceptin-refractory HER2-positive metastatic breast cancer, the mechanism is purported to be an antiproliferative effect relating to the synergism of these two agents.">
        <entity id="DDI-MedLine.d164.s1.e0" charOffset="41-49"
            type="drug" text="lapatinib"/>
        <entity id="DDI-MedLine.d164.s1.e1" charOffset="65-73"
            type="brand" text="Herceptin"/>
        <entity id="DDI-MedLine.d164.s1.e2" charOffset="133-141"
            type="drug" text="lapatinib"/>
