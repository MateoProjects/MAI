<document id="DDI-DrugBank.d546">
    <sentence id="DDI-DrugBank.d546.s0" text="Drug Interactions with Beta-Blockers: Concomitant use of fenoldopam with beta-blockers should be avoided.">
        <entity id="DDI-DrugBank.d546.s0.e0" charOffset="23-35"
            type="group" text="Beta-Blockers"/>
        <entity id="DDI-DrugBank.d546.s0.e1" charOffset="57-66"
            type="drug" text="fenoldopam"/>
        <entity id="DDI-DrugBank.d546.s0.e2" charOffset="73-85"
            type="group" text="beta-blockers"/>
        <pair id="DDI-DrugBank.d546.s0.p0" e1="DDI-DrugBank.d546.s0.e0"
