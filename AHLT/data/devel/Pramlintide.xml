        <pair id="DDI-DrugBank.d632.s6.p2" e1="DDI-DrugBank.d632.s6.e0"
            e2="DDI-DrugBank.d632.s6.e3" ddi="false"/>
        <pair id="DDI-DrugBank.d632.s6.p3" e1="DDI-DrugBank.d632.s6.e1"
            e2="DDI-DrugBank.d632.s6.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d632.s6.p4" e1="DDI-DrugBank.d632.s6.e1"
