    </sentence>
    <sentence id="DDI-DrugBank.d419.s2" text="In controlled clinical trials of AUGMENTIN XR, 22 patients received concomitant allopurinol and AUGMENTIN XR.">
        <entity id="DDI-DrugBank.d419.s2.e0" charOffset="33-44"
            type="brand" text="AUGMENTIN XR"/>
        <entity id="DDI-DrugBank.d419.s2.e1" charOffset="80-90"
            type="drug" text="allopurinol"/>
        <entity id="DDI-DrugBank.d419.s2.e2" charOffset="96-107"
