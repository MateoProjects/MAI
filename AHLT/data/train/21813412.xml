            e2="DDI-MedLine.d194.s10.e3" ddi="false"/>
        <pair id="DDI-MedLine.d194.s10.p6" e1="DDI-MedLine.d194.s10.e1"
            e2="DDI-MedLine.d194.s10.e4" ddi="false"/>
        <pair id="DDI-MedLine.d194.s10.p7" e1="DDI-MedLine.d194.s10.e2"
