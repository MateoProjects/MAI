    </sentence>
    <sentence id="DDI-DrugBank.d422.s4" text="When digoxin and BREVIBLOC were concomitantly administered intravenously to normal volunteers, there was a 10-20% increase in digoxin blood levels at some time points.">
        <entity id="DDI-DrugBank.d422.s4.e0" charOffset="5-11"
            type="drug" text="digoxin"/>
        <entity id="DDI-DrugBank.d422.s4.e1" charOffset="17-25"
            type="brand" text="BREVIBLOC"/>
        <entity id="DDI-DrugBank.d422.s4.e2" charOffset="126-132"
            type="drug" text="digoxin"/>
        <pair id="DDI-DrugBank.d422.s4.p0" e1="DDI-DrugBank.d422.s4.e0"
