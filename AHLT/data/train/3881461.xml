    <sentence id="DDI-MedLine.d12.s1" text="Prostatic epithelium proliferates in a defined medium consisting of basal medium RPMI1640 containing transferrin (1 microgram/ml), EGF (10 ng/ml), and insulin (3.7 micrograms/ml or 0.1 IU/ml). ">
        <entity id="DDI-MedLine.d12.s1.e0" charOffset="101-111"
            type="drug_n" text="transferrin"/>
        <entity id="DDI-MedLine.d12.s1.e1" charOffset="131-133"
            type="drug_n" text="EGF"/>
        <entity id="DDI-MedLine.d12.s1.e2" charOffset="151-157"
            type="drug" text="insulin"/>
