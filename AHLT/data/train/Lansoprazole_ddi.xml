    <sentence id="DDI-DrugBank.d431.s0" text="Lansoprazole is metabolized through the cytochrome P450 system, specifically through the CYP3A and CYP2C19 isozymes.">
        <entity id="DDI-DrugBank.d431.s0.e0" charOffset="0-11"
            type="drug" text="Lansoprazole"/>
    </sentence>
