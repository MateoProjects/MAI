    </sentence>
    <sentence id="DDI-DrugBank.d431.s9" text="In clinical trials, antacids were administered concomitantly with lansoprazole delayed-release capsules;">
        <entity id="DDI-DrugBank.d431.s9.e0" charOffset="20-27"
            type="group" text="antacids"/>
        <entity id="DDI-DrugBank.d431.s9.e1" charOffset="66-77"
