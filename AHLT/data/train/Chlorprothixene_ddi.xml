<document id="DDI-DrugBank.d503">
    <sentence id="DDI-DrugBank.d503.s0" text="Chlorprothixene may increase the plasma-level of concomitantly given lithium.">
        <entity id="DDI-DrugBank.d503.s0.e0" charOffset="0-14"
            type="drug" text="Chlorprothixene"/>
        <entity id="DDI-DrugBank.d503.s0.e1" charOffset="69-75"
            type="drug" text="lithium"/>
        <pair id="DDI-DrugBank.d503.s0.p0" e1="DDI-DrugBank.d503.s0.e0"
            e2="DDI-DrugBank.d503.s0.e1" ddi="true" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d503.s1" text="In order to avoid lithium intoxication, lithium plasma levels should be monitored closely.">
        <entity id="DDI-DrugBank.d503.s1.e0" charOffset="18-24"
            type="drug" text="lithium"/>
        <entity id="DDI-DrugBank.d503.s1.e1" charOffset="40-46"
            type="drug" text="lithium"/>
        <pair id="DDI-DrugBank.d503.s1.p0" e1="DDI-DrugBank.d503.s1.e0"
            e2="DDI-DrugBank.d503.s1.e1" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d503.s2" text="If chlorprothixene is given concomitantly with opioids, the opioid dose should be reduced (by approx. 50%), because chlorprothixene amplifies the therapeutic actions and side-effects of opioids massively.">
        <entity id="DDI-DrugBank.d503.s2.e0" charOffset="3-17"
            type="drug" text="chlorprothixene"/>
        <entity id="DDI-DrugBank.d503.s2.e1" charOffset="47-53"
            type="group" text="opioids"/>
        <entity id="DDI-DrugBank.d503.s2.e2" charOffset="60-65"
            type="group" text="opioid"/>
        <entity id="DDI-DrugBank.d503.s2.e3" charOffset="116-130"
            type="drug" text="chlorprothixene"/>
        <entity id="DDI-DrugBank.d503.s2.e4" charOffset="186-192"
            type="group" text="opioids"/>
        <pair id="DDI-DrugBank.d503.s2.p0" e1="DDI-DrugBank.d503.s2.e0"
            e2="DDI-DrugBank.d503.s2.e1" ddi="true" type="advise"/>
        <pair id="DDI-DrugBank.d503.s2.p1" e1="DDI-DrugBank.d503.s2.e0"
            e2="DDI-DrugBank.d503.s2.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d503.s2.p2" e1="DDI-DrugBank.d503.s2.e0"
            e2="DDI-DrugBank.d503.s2.e3" ddi="false"/>
        <pair id="DDI-DrugBank.d503.s2.p3" e1="DDI-DrugBank.d503.s2.e0"
            e2="DDI-DrugBank.d503.s2.e4" ddi="false"/>
        <pair id="DDI-DrugBank.d503.s2.p4" e1="DDI-DrugBank.d503.s2.e1"
            e2="DDI-DrugBank.d503.s2.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d503.s2.p5" e1="DDI-DrugBank.d503.s2.e1"
            e2="DDI-DrugBank.d503.s2.e3" ddi="false"/>
        <pair id="DDI-DrugBank.d503.s2.p6" e1="DDI-DrugBank.d503.s2.e1"
            e2="DDI-DrugBank.d503.s2.e4" ddi="false"/>
        <pair id="DDI-DrugBank.d503.s2.p7" e1="DDI-DrugBank.d503.s2.e2"
