    <sentence id="DDI-DrugBank.d350.s0" text="Studies in vitro show that caspofungin acetate is not an inhibitor of any enzyme in the cytochrome P450 (CYP) system.">
        <entity id="DDI-DrugBank.d350.s0.e0" charOffset="27-45"
            type="drug" text="caspofungin acetate"/>
    </sentence>
    <sentence id="DDI-DrugBank.d350.s1" text="In clinical studies, caspofungin did not induce the CYP3A4 metabolism of other drugs.">
        <entity id="DDI-DrugBank.d350.s1.e0" charOffset="21-31"
            type="drug" text="caspofungin"/>
    </sentence>
    <sentence id="DDI-DrugBank.d350.s2" text="Caspofungin is not a substrate for P-glycoprotein and is a poor substrate for cytochrome P450 enzymes.">
        <entity id="DDI-DrugBank.d350.s2.e0" charOffset="0-10"
            type="drug" text="Caspofungin"/>
    </sentence>
    <sentence id="DDI-DrugBank.d350.s3" text="Clinical studies in healthy volunteers show that the pharmacokinetics of CANCIDAS are not altered by itraconazole, amphotericin B, mycophenolate, nelfinavir, or tacrolimus.">
