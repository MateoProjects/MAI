        <pair id="DDI-MedLine.d58.s7.p13" e1="DDI-MedLine.d58.s7.e3"
            e2="DDI-MedLine.d58.s7.e5" ddi="false"/>
        <pair id="DDI-MedLine.d58.s7.p14" e1="DDI-MedLine.d58.s7.e4"
            e2="DDI-MedLine.d58.s7.e5" ddi="false"/>
    </sentence>
    <sentence id="DDI-MedLine.d58.s8" text="Thus, the selective regulation of voltage-dependent Ca(2+) subtypes by ginsenosides in bovine chromaffin cell could be the cellular basis of antistress effects induced by ginseng.">
        <entity id="DDI-MedLine.d58.s8.e0" charOffset="71-82"
            type="drug_n" text="ginsenosides"/>
        <entity id="DDI-MedLine.d58.s8.e1" charOffset="171-177"
