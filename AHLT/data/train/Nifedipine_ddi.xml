    <sentence id="DDI-DrugBank.d373.s3" text="Extended Release Tablets: Administration of nifedipine with digoxin increased digoxin levels in 9 of 12 normal volunteers.">
        <entity id="DDI-DrugBank.d373.s3.e0" charOffset="44-53"
            type="drug" text="nifedipine"/>
        <entity id="DDI-DrugBank.d373.s3.e1" charOffset="60-66"
            type="drug" text="digoxin"/>
        <entity id="DDI-DrugBank.d373.s3.e2" charOffset="78-84"
            type="drug" text="digoxin"/>
        <pair id="DDI-DrugBank.d373.s3.p0" e1="DDI-DrugBank.d373.s3.e0"
            e2="DDI-DrugBank.d373.s3.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d373.s3.p1" e1="DDI-DrugBank.d373.s3.e0"
