        <entity id="DDI-DrugBank.d364.s10.e3" charOffset="99-117"
            type="group" text="protease inhibitors"/>
        <entity id="DDI-DrugBank.d364.s10.e4" charOffset="120-131"
            type="drug" text="voriconazole"/>
