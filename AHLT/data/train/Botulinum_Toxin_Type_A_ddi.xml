    <sentence id="DDI-DrugBank.d133.s1" text="The effect of administering different botulinum neurotoxin serotypes at the same time or within several months of each other is unknown.">
        <entity id="DDI-DrugBank.d133.s1.e0" charOffset="38-57"
            type="group" text="botulinum neurotoxin"/>
