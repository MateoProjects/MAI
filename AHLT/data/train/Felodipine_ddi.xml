        <pair id="DDI-DrugBank.d316.s3.p0" e1="DDI-DrugBank.d316.s3.e0"
            e2="DDI-DrugBank.d316.s3.e1" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d316.s4" text="Caution should be used when CYP3A4 inhibitors are co-administered with felodipine.">
        <entity id="DDI-DrugBank.d316.s4.e0" charOffset="28-33"
            type="drug" text="CYP3A4"/>
        <entity id="DDI-DrugBank.d316.s4.e1" charOffset="71-80"
            type="drug" text="felodipine"/>
        <pair id="DDI-DrugBank.d316.s4.p0" e1="DDI-DrugBank.d316.s4.e0"
            e2="DDI-DrugBank.d316.s4.e1" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d316.s5" text="A conservative approach to dosing felodipine should be taken.">
        <entity id="DDI-DrugBank.d316.s5.e0" charOffset="34-43"
            type="drug" text="felodipine"/>
    </sentence>
    <sentence id="DDI-DrugBank.d316.s6" text="The following specific interactions have been reported: Itraconazole Co-administration of another extended release formulation of felodipine with itraconazole resulted in approximately 8-fold increase in the AUC, more than 6- fold increase in the Cmax, and 2-fold prolongation in the half- life of felodipine.">
        <entity id="DDI-DrugBank.d316.s6.e0" charOffset="130-139"
            type="drug" text="felodipine"/>
        <entity id="DDI-DrugBank.d316.s6.e1" charOffset="146-157"
            type="drug" text="itraconazole"/>
        <entity id="DDI-DrugBank.d316.s6.e2" charOffset="298-307"
            type="drug" text="felodipine"/>
        <pair id="DDI-DrugBank.d316.s6.p0" e1="DDI-DrugBank.d316.s6.e0"
            e2="DDI-DrugBank.d316.s6.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d316.s6.p1" e1="DDI-DrugBank.d316.s6.e0"
            e2="DDI-DrugBank.d316.s6.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d316.s6.p2" e1="DDI-DrugBank.d316.s6.e1"
            e2="DDI-DrugBank.d316.s6.e2" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d316.s7" text="Erythromycin Co-administration of felodipine (PLENDIL) with erythromycin resulted in approximately 2.5- fold increase in the AUC and Cmax, and about 2- fold prolongation in the half- life of felodipine.">
        <entity id="DDI-DrugBank.d316.s7.e0" charOffset="34-43"
            type="drug" text="felodipine"/>
        <entity id="DDI-DrugBank.d316.s7.e1" charOffset="46-52"
            type="drug" text="PLENDIL"/>
        <entity id="DDI-DrugBank.d316.s7.e2" charOffset="60-71"
            type="drug" text="erythromycin"/>
        <entity id="DDI-DrugBank.d316.s7.e3" charOffset="191-200"
            type="drug" text="felodipine"/>
        <pair id="DDI-DrugBank.d316.s7.p0" e1="DDI-DrugBank.d316.s7.e0"
            e2="DDI-DrugBank.d316.s7.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d316.s7.p1" e1="DDI-DrugBank.d316.s7.e0"
            e2="DDI-DrugBank.d316.s7.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d316.s7.p2" e1="DDI-DrugBank.d316.s7.e0"
            e2="DDI-DrugBank.d316.s7.e3" ddi="false"/>
        <pair id="DDI-DrugBank.d316.s7.p3" e1="DDI-DrugBank.d316.s7.e1"
