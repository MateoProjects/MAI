            type="drug" text="theophylline"/>
        <entity id="DDI-MedLine.d51.s3.e1" charOffset="73-81"
            type="drug_n" text="CGS 21680"/>
        <entity id="DDI-MedLine.d51.s3.e2" charOffset="120-142"
            type="drug_n" text="N6-cyclopentyladenosine"/>
        <entity id="DDI-MedLine.d51.s3.e3" charOffset="145-147"
            type="drug_n" text="CPA"/>
        <pair id="DDI-MedLine.d51.s3.p0" e1="DDI-MedLine.d51.s3.e0"
            e2="DDI-MedLine.d51.s3.e1" ddi="true" type="effect"/>
        <pair id="DDI-MedLine.d51.s3.p1" e1="DDI-MedLine.d51.s3.e0"
