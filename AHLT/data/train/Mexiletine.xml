    </sentence>
    <sentence id="DDI-DrugBank.d633.s13" text="Concurrent administration of cimetidine and Mexitil    has been reported to increase, decrease, or leave unchanged Mexitil    plasma levels; ">
        <entity id="DDI-DrugBank.d633.s13.e0" charOffset="29-38"
            type="drug" text="cimetidine"/>
        <entity id="DDI-DrugBank.d633.s13.e1" charOffset="44-50"
            type="brand" text="Mexitil"/>
        <entity id="DDI-DrugBank.d633.s13.e2" charOffset="115-121"
            type="brand" text="Mexitil"/>
        <pair id="DDI-DrugBank.d633.s13.p0"
            e1="DDI-DrugBank.d633.s13.e0" e2="DDI-DrugBank.d633.s13.e1"
            ddi="true" type="mechanism"/>
        <pair id="DDI-DrugBank.d633.s13.p1"
            e1="DDI-DrugBank.d633.s13.e0" e2="DDI-DrugBank.d633.s13.e2" ddi="false"/>
