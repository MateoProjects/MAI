    <sentence id="DDI-DrugBank.d440.s4" text="Of over 3500 patients enrolled in the Ibandronate osteoporosis Treatment and Prevention Studies, 15% used anti-peptic agents (primarily H2 blockers and PPIs).">
        <entity id="DDI-DrugBank.d440.s4.e0" charOffset="38-48"
            type="drug" text="Ibandronate"/>
        <entity id="DDI-DrugBank.d440.s4.e1" charOffset="136-146"
            type="group" text="H2 blockers"/>
        <entity id="DDI-DrugBank.d440.s4.e2" charOffset="152-155"
            type="group" text="PPIs"/>
        <pair id="DDI-DrugBank.d440.s4.p0" e1="DDI-DrugBank.d440.s4.e0"
