            type="drug" text="meropenem"/>
        <pair id="DDI-DrugBank.d762.s0.p0" e1="DDI-DrugBank.d762.s0.e0"
            e2="DDI-DrugBank.d762.s0.e1" ddi="true" type="mechanism"/>
        <pair id="DDI-DrugBank.d762.s0.p1" e1="DDI-DrugBank.d762.s0.e0"
            e2="DDI-DrugBank.d762.s0.e2" ddi="false"/>
