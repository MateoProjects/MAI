    <sentence id="DDI-DrugBank.d762.s0" text="Probenecid competes with meropenem for active tubular secretion and thus inhibits the renal excretion of meropenem. ">
        <entity id="DDI-DrugBank.d762.s0.e0" charOffset="0-9"
            type="drug" text="Probenecid"/>
        <entity id="DDI-DrugBank.d762.s0.e1" charOffset="25-33"
            type="drug" text="meropenem"/>
