    <sentence id="DDI-DrugBank.d84.s4" text="The concomitant administration of Exjade and vitamin C has not been formally studied.">
        <entity id="DDI-DrugBank.d84.s4.e0" charOffset="34-39"
            type="brand" text="Exjade"/>
        <entity id="DDI-DrugBank.d84.s4.e1" charOffset="45-53"
