        <pair id="DDI-MedLine.d195.s4.p4" e1="DDI-MedLine.d195.s4.e1"
            e2="DDI-MedLine.d195.s4.e3" ddi="false"/>
        <pair id="DDI-MedLine.d195.s4.p5" e1="DDI-MedLine.d195.s4.e2"
            e2="DDI-MedLine.d195.s4.e3" ddi="false"/>
    </sentence>
    <sentence id="DDI-MedLine.d195.s5" text="Ibuprofen plasma concentration was also increased when it was administered with piperine. ">
        <entity id="DDI-MedLine.d195.s5.e0" charOffset="0-8" type="drug" text="Ibuprofen"/>
        <entity id="DDI-MedLine.d195.s5.e1" charOffset="80-87"
            type="drug_n" text="piperine"/>
        <pair id="DDI-MedLine.d195.s5.p0" e1="DDI-MedLine.d195.s5.e0"
            e2="DDI-MedLine.d195.s5.e1" ddi="true" type="mechanism"/>
    </sentence>
    <sentence id="DDI-MedLine.d195.s6" text="The synergistic antinociception activity of ibuprofen when administered with piperine can be attributed to increased plasma concentration of ibuprofen. ">
        <entity id="DDI-MedLine.d195.s6.e0" charOffset="44-52"
            type="drug" text="ibuprofen"/>
        <entity id="DDI-MedLine.d195.s6.e1" charOffset="77-84"
