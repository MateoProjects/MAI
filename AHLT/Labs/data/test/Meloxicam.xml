        <entity id="DDI-DrugBank.d597.s14.e3" charOffset="145-162"
            type="group" text="thiazide diuretics"/>
        <pair id="DDI-DrugBank.d597.s14.p0"
            e1="DDI-DrugBank.d597.s14.e0" e2="DDI-DrugBank.d597.s14.e1" ddi="false"/>
