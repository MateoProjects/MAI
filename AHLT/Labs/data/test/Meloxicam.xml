            e1="DDI-DrugBank.d597.s16.e0" e2="DDI-DrugBank.d597.s16.e1" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d597.s17" text="Furosemide: single and multiple dose pharmacodynamics and pharmacokinetics are not affected by multiple doses of meloxicam. ">
        <entity id="DDI-DrugBank.d597.s17.e0" charOffset="0-9"
            type="drug" text="Furosemide"/>
        <entity id="DDI-DrugBank.d597.s17.e1" charOffset="113-121"
