            e1="DDI-DrugBank.d597.s20.e2" e2="DDI-DrugBank.d597.s20.e3" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d597.s21" text="These effects have been attributed to inhibition of renal prostaglandin synthesis by MOBIC. ">
        <entity id="DDI-DrugBank.d597.s21.e0" charOffset="85-89"
            type="brand" text="MOBIC"/>
    </sentence>
    <sentence id="DDI-DrugBank.d597.s22" text="Patients on lithium treatment should be closely monitored when MOBIC is introduced or withdrawn. ">
        <entity id="DDI-DrugBank.d597.s22.e0" charOffset="12-18"
            type="drug" text="lithium"/>
        <entity id="DDI-DrugBank.d597.s22.e1" charOffset="63-67"
