            ddi="true" type="effect"/>
        <pair id="DDI-DrugBank.d597.s14.p5"
            e1="DDI-DrugBank.d597.s14.e2" e2="DDI-DrugBank.d597.s14.e3" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d597.s15" text="This effect has been attributed to inhibition of renal prostaglandin synthesis. "/>
    <sentence id="DDI-DrugBank.d597.s16" text="Studies with furosemide agents and meloxicam have not demonstrated a reduction in natriuretic effect. ">
        <entity id="DDI-DrugBank.d597.s16.e0" charOffset="13-22"
            type="drug" text="furosemide"/>
