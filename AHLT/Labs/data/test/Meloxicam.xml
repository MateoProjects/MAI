        <pair id="DDI-DrugBank.d597.s23.p0"
            e1="DDI-DrugBank.d597.s23.e0" e2="DDI-DrugBank.d597.s23.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d597.s23.p1"
            e1="DDI-DrugBank.d597.s23.e0" e2="DDI-DrugBank.d597.s23.e2" ddi="false"/>
