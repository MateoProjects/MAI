        <pair id="DDI-DrugBank.d597.s26.p0"
            e1="DDI-DrugBank.d597.s26.e0" e2="DDI-DrugBank.d597.s26.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d597.s26.p1"
            e1="DDI-DrugBank.d597.s26.e0" e2="DDI-DrugBank.d597.s26.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d597.s26.p2"
            e1="DDI-DrugBank.d597.s26.e1" e2="DDI-DrugBank.d597.s26.e2"
            ddi="true" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d597.s27" text="The effect of meloxicam on the anticoagulant effect of warfarin was studied in a group of healthy subjects receiving daily doses of warfarin that produced an INR (International Normalized Ratio) between 1.2 and 1.8. ">
