    </sentence>
    <sentence id="DDI-DrugBank.d597.s7" text="Cholestyramine: Pretreatment for four days with cholestyramine significantly increased the clearance of meloxicam by 50%. ">
        <entity id="DDI-DrugBank.d597.s7.e0" charOffset="0-13"
            type="drug" text="Cholestyramine"/>
