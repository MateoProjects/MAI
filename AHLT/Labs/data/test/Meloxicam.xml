    </sentence>
    <sentence id="DDI-DrugBank.d597.s25" text="In vitro, methotrexate did not displace meloxicam from its human serum binding sites. ">
        <entity id="DDI-DrugBank.d597.s25.e0" charOffset="10-21"
            type="drug" text="methotrexate"/>
        <entity id="DDI-DrugBank.d597.s25.e1" charOffset="40-48"
