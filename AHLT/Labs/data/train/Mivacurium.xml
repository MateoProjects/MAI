            type="drug" text="Isoflurane"/>
        <entity id="DDI-DrugBank.d775.s5.e1" charOffset="15-23"
            type="drug" text="enflurane"/>
        <entity id="DDI-DrugBank.d775.s5.e2" charOffset="44-56"
            type="drug" text="nitrous oxide"/>
        <entity id="DDI-DrugBank.d775.s5.e3" charOffset="58-63"
            type="drug" text="oxygen"/>
        <pair id="DDI-DrugBank.d775.s5.p0" e1="DDI-DrugBank.d775.s5.e0"
