<document id="DDI-DrugBank.d775">
    <sentence id="DDI-DrugBank.d775.s0" text="Although MIVACRON (a mixture of three stereoisomers) has been administered safely following succinylcholine-facilitated tracheal intubation, the interaction between MIVACRON and succinylcholine has not been systematically studied. ">
        <entity id="DDI-DrugBank.d775.s0.e0" charOffset="9-16"
            type="brand" text="MIVACRON"/>
        <entity id="DDI-DrugBank.d775.s0.e1" charOffset="165-172"
            type="brand" text="MIVACRON"/>
        <entity id="DDI-DrugBank.d775.s0.e2" charOffset="178-192"
            type="drug" text="succinylcholine"/>
        <pair id="DDI-DrugBank.d775.s0.p0" e1="DDI-DrugBank.d775.s0.e0"
            e2="DDI-DrugBank.d775.s0.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d775.s0.p1" e1="DDI-DrugBank.d775.s0.e0"
            e2="DDI-DrugBank.d775.s0.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d775.s0.p2" e1="DDI-DrugBank.d775.s0.e1"
            e2="DDI-DrugBank.d775.s0.e2" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d775.s1" text="Prior administration of succinylcholine can potentiate the neuromuscular blocking effects of nondepolarizing agents. ">
        <entity id="DDI-DrugBank.d775.s1.e0" charOffset="24-38"
            type="drug" text="succinylcholine"/>
        <entity id="DDI-DrugBank.d775.s1.e1" charOffset="93-114"
            type="group" text="nondepolarizing agents"/>
        <pair id="DDI-DrugBank.d775.s1.p0" e1="DDI-DrugBank.d775.s1.e0"
            e2="DDI-DrugBank.d775.s1.e1" ddi="true" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d775.s2" text="Evidence of spontaneous recovery from succinylcholine should be observed before the administration of MIVACRON. ">
        <entity id="DDI-DrugBank.d775.s2.e0" charOffset="38-52"
            type="drug" text="succinylcholine"/>
        <entity id="DDI-DrugBank.d775.s2.e1" charOffset="102-109"
            type="brand" text="MIVACRON"/>
        <pair id="DDI-DrugBank.d775.s2.p0" e1="DDI-DrugBank.d775.s2.e0"
            e2="DDI-DrugBank.d775.s2.e1" ddi="true" type="advise"/>
    </sentence>
    <sentence id="DDI-DrugBank.d775.s3" text="The use of MIVACRON before succinylcholine to attenuate some of the side effects of succinylcholine has not been studied. ">
        <entity id="DDI-DrugBank.d775.s3.e0" charOffset="11-18"
            type="brand" text="MIVACRON"/>
        <entity id="DDI-DrugBank.d775.s3.e1" charOffset="27-41"
            type="drug" text="succinylcholine"/>
        <entity id="DDI-DrugBank.d775.s3.e2" charOffset="84-98"
            type="drug" text="succinylcholine"/>
        <pair id="DDI-DrugBank.d775.s3.p0" e1="DDI-DrugBank.d775.s3.e0"
            e2="DDI-DrugBank.d775.s3.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d775.s3.p1" e1="DDI-DrugBank.d775.s3.e0"
            e2="DDI-DrugBank.d775.s3.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d775.s3.p2" e1="DDI-DrugBank.d775.s3.e1"
            e2="DDI-DrugBank.d775.s3.e2" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d775.s4" text="There are no clinical data on the use of MIVACRON with other nondepolarizing neuromuscular blocking agents. ">
        <entity id="DDI-DrugBank.d775.s4.e0" charOffset="41-48"
            type="brand" text="MIVACRON"/>
        <entity id="DDI-DrugBank.d775.s4.e1" charOffset="61-105"
            type="group" text="nondepolarizing neuromuscular blocking agents"/>
        <pair id="DDI-DrugBank.d775.s4.p0" e1="DDI-DrugBank.d775.s4.e0"
            e2="DDI-DrugBank.d775.s4.e1" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d775.s5" text="Isoflurane and enflurane (administered with nitrous oxide/oxygen to achieve 1.25 M.C. ">
