        <pair id="DDI-DrugBank.d775.s5.p5" e1="DDI-DrugBank.d775.s5.e2"
            e2="DDI-DrugBank.d775.s5.e3" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d775.s6" text="decrease the ED50 of MIVACRON by as much as 25% (see CLINICAL PHARMACOLOGY: Pharmacodynamics and Individualization of Dosages). ">
        <entity id="DDI-DrugBank.d775.s6.e0" charOffset="21-28"
            type="brand" text="MIVACRON"/>
    </sentence>
    <sentence id="DDI-DrugBank.d775.s7" text="These agents may also prolong the clinically effective duration of action and decrease the average infusion requirement of MIVACRON by as much as 35% to 40%. ">
        <entity id="DDI-DrugBank.d775.s7.e0" charOffset="123-130"
            type="brand" text="MIVACRON"/>
    </sentence>
    <sentence id="DDI-DrugBank.d775.s8" text="A greater potentiation of the neuromuscular blocking effects of MIVACRON may be expected with higher concentrations of enflurane or isoflurane. ">
        <entity id="DDI-DrugBank.d775.s8.e0" charOffset="64-71"
            type="brand" text="MIVACRON"/>
        <entity id="DDI-DrugBank.d775.s8.e1" charOffset="119-127"
            type="drug" text="enflurane"/>
        <entity id="DDI-DrugBank.d775.s8.e2" charOffset="132-141"
            type="drug" text="isoflurane"/>
        <pair id="DDI-DrugBank.d775.s8.p0" e1="DDI-DrugBank.d775.s8.e0"
            e2="DDI-DrugBank.d775.s8.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d775.s8.p1" e1="DDI-DrugBank.d775.s8.e0"
            e2="DDI-DrugBank.d775.s8.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d775.s8.p2" e1="DDI-DrugBank.d775.s8.e1"
            e2="DDI-DrugBank.d775.s8.e2" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d775.s9" text="Halothane has little or no effect on the ED50 , but may prolong the duration of action and decrease the average infusion requirement by as much as 20%. ">
        <entity id="DDI-DrugBank.d775.s9.e0" charOffset="0-8"
            type="drug" text="Halothane"/>
    </sentence>
    <sentence id="DDI-DrugBank.d775.s10" text="Other drugs which may enhance the neuromuscular blocking action of nondepolarizing agents such as MIVACRON include certain antibiotics (e.g., aminoglycosides, tetracyclines, bacitracin, polymyxins, lincomycin, clindamycin, colistin, and sodium colistimethate), magnesium salts, lithium, local anesthetics, procainamide, and quinidine. ">
