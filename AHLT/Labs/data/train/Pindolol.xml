    <sentence id="DDI-DrugBank.d666.s0" text="Catecholamine-depleting drugs (e.g., reserpine) may have an additive effect when given with beta-blocking agents. ">
        <entity id="DDI-DrugBank.d666.s0.e0" charOffset="37-45"
            type="drug" text="reserpine"/>
        <entity id="DDI-DrugBank.d666.s0.e1" charOffset="92-111"
            type="group" text="beta-blocking agents"/>
        <pair id="DDI-DrugBank.d666.s0.p0" e1="DDI-DrugBank.d666.s0.e0"
            e2="DDI-DrugBank.d666.s0.e1" ddi="true" type="effect"/>
    </sentence>
    <sentence id="DDI-DrugBank.d666.s1" text="Patients receiving pindolol plus a catecholamine-depleting agent should, therefore, be closely observed for evidence of hypotension and/or marked bradycardia which may produce vertigo, syncope, or postural hypotension. ">
        <entity id="DDI-DrugBank.d666.s1.e0" charOffset="19-26"
            type="drug" text="pindolol"/>
    </sentence>
    <sentence id="DDI-DrugBank.d666.s2" text="Pindolol has been used with a variety of antihypertensive agents, including hydrochlorothiazide, hydralazine, and guanethidine without unexpected adverse interactions. ">
        <entity id="DDI-DrugBank.d666.s2.e0" charOffset="0-7"
            type="drug" text="Pindolol"/>
        <entity id="DDI-DrugBank.d666.s2.e1" charOffset="41-63"
            type="group" text="antihypertensive agents"/>
        <entity id="DDI-DrugBank.d666.s2.e2" charOffset="76-94"
            type="drug" text="hydrochlorothiazide"/>
        <entity id="DDI-DrugBank.d666.s2.e3" charOffset="97-107"
            type="drug" text="hydralazine"/>
        <entity id="DDI-DrugBank.d666.s2.e4" charOffset="114-125"
            type="drug" text="guanethidine"/>
        <pair id="DDI-DrugBank.d666.s2.p0" e1="DDI-DrugBank.d666.s2.e0"
            e2="DDI-DrugBank.d666.s2.e1" ddi="false"/>
