    <sentence id="DDI-DrugBank.d386.s25" text="5."/>
    <sentence id="DDI-DrugBank.d386.s26" text="Concurrent administration of cimetidine and tricyclic antidepressants can produce clinically significant increases in the plasma levels of the tricyclic antidepressants.">
        <entity id="DDI-DrugBank.d386.s26.e0" charOffset="29-38"
            type="drug" text="cimetidine"/>
        <entity id="DDI-DrugBank.d386.s26.e1" charOffset="44-68"
            type="group" text="tricyclic antidepressants"/>
        <entity id="DDI-DrugBank.d386.s26.e2" charOffset="143-167"
            type="group" text="tricyclic antidepressants"/>
        <pair id="DDI-DrugBank.d386.s26.p0"
