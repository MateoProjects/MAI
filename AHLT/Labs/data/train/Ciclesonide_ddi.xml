        <pair id="DDI-DrugBank.d362.s2.p2" e1="DDI-DrugBank.d362.s2.e1"
            e2="DDI-DrugBank.d362.s2.e2" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d362.s3" text="In a drug interaction study, co-administration of orally inhaled ciclesonide and oral erythromycin, an inhibitor of cytochrome P450 3A4, had no effect on the pharmacokinetics of either des-ciclesonide or erythromycin.">
        <entity id="DDI-DrugBank.d362.s3.e0" charOffset="65-75"
            type="drug" text="ciclesonide"/>
        <entity id="DDI-DrugBank.d362.s3.e1" charOffset="86-97"
