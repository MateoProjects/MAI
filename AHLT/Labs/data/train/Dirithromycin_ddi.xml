        <entity id="DDI-DrugBank.d522.s9.e2" charOffset="88-100"
            type="drug" text="dirithromycin"/>
        <pair id="DDI-DrugBank.d522.s9.p0" e1="DDI-DrugBank.d522.s9.e0"
