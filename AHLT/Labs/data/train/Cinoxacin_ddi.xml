    </sentence>
    <sentence id="DDI-DrugBank.d562.s9" text="When these products are administered concomitantly, prothrombin time or other suitable coagulation tests should be closely monitored."/>
    <sentence id="DDI-DrugBank.d562.s10" text="Seizures have been reported in patients taking another quinolone class antimicrobial and the nonsteroidal anti-inflammatory drug fenbufen concurrently.">
        <entity id="DDI-DrugBank.d562.s10.e0" charOffset="55-83"
            type="group" text="quinolone class antimicrobial"/>
        <entity id="DDI-DrugBank.d562.s10.e1" charOffset="93-127"
            type="group" text="nonsteroidal anti-inflammatory drug"/>
        <entity id="DDI-DrugBank.d562.s10.e2" charOffset="129-136"
            type="drug" text="fenbufen"/>
        <pair id="DDI-DrugBank.d562.s10.p0"
            e1="DDI-DrugBank.d562.s10.e0" e2="DDI-DrugBank.d562.s10.e1"
            ddi="true" type="effect"/>
        <pair id="DDI-DrugBank.d562.s10.p1"
            e1="DDI-DrugBank.d562.s10.e0" e2="DDI-DrugBank.d562.s10.e2"
