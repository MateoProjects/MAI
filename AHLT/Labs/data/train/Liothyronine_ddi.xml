        <entity id="DDI-DrugBank.d54.s21.e2" charOffset="82-92"
            type="drug" text="epinephrine"/>
        <entity id="DDI-DrugBank.d54.s21.e3" charOffset="98-111"
            type="drug" text="norepinephrine"/>
        <pair id="DDI-DrugBank.d54.s21.p0" e1="DDI-DrugBank.d54.s21.e0"
