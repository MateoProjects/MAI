        <pair id="DDI-DrugBank.d54.s21.p2" e1="DDI-DrugBank.d54.s21.e0"
            e2="DDI-DrugBank.d54.s21.e3" ddi="false"/>
        <pair id="DDI-DrugBank.d54.s21.p3" e1="DDI-DrugBank.d54.s21.e1"
