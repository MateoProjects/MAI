            type="drug" text="Ketamine"/>
        <entity id="DDI-DrugBank.d54.s19.e1" charOffset="45-63"
            type="group" text="thyroid preparation"/>
        <entity id="DDI-DrugBank.d54.s19.e2" charOffset="82-91"
            type="group" text="anesthetic"/>
        <pair id="DDI-DrugBank.d54.s19.p0" e1="DDI-DrugBank.d54.s19.e0"
            e2="DDI-DrugBank.d54.s19.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d54.s19.p1" e1="DDI-DrugBank.d54.s19.e0"
