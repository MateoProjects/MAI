            type="drug" text="imipramine"/>
        <entity id="DDI-DrugBank.d54.s15.e3" charOffset="77-101"
            type="group" text="tricyclic antidepressants"/>
        <pair id="DDI-DrugBank.d54.s15.p0" e1="DDI-DrugBank.d54.s15.e0"
            e2="DDI-DrugBank.d54.s15.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d54.s15.p1" e1="DDI-DrugBank.d54.s15.e0"
