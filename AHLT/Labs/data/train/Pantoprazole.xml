    <sentence id="DDI-DrugBank.d680.s2" text="Clinically relevant interactions of pantoprazole with other drugs with the same metabolic pathways are not expected. ">
        <entity id="DDI-DrugBank.d680.s2.e0" charOffset="36-47"
            type="drug" text="pantoprazole"/>
    </sentence>
    <sentence id="DDI-DrugBank.d680.s3" text="Therefore, when coadministered with pantoprazole, adjustment of the dosage of pantoprazole or of such drugs may not be necessary. ">
        <entity id="DDI-DrugBank.d680.s3.e0" charOffset="36-47"
            type="drug" text="pantoprazole"/>
        <entity id="DDI-DrugBank.d680.s3.e1" charOffset="78-89"
            type="drug" text="pantoprazole"/>
        <pair id="DDI-DrugBank.d680.s3.p0" e1="DDI-DrugBank.d680.s3.e0"
            e2="DDI-DrugBank.d680.s3.e1" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d680.s4" text="There was also no interaction with concomitantly administered antacids. ">
        <entity id="DDI-DrugBank.d680.s4.e0" charOffset="62-69"
            type="group" text="antacids"/>
    </sentence>
    <sentence id="DDI-DrugBank.d680.s5" text="There have been postmarketing reports of increased INR and prothrombin time in patients receiving proton pump inhibitors, including pantoprazole, and warfarin concomitantly. ">
        <entity id="DDI-DrugBank.d680.s5.e0" charOffset="98-119"
            type="group" text="proton pump inhibitors"/>
