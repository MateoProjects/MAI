    <sentence id="DDI-DrugBank.d543.s2" text="Anticholinergics antagonize the effects of antiglaucoma agents.">
        <entity id="DDI-DrugBank.d543.s2.e0" charOffset="0-15"
            type="group" text="Anticholinergics"/>
        <entity id="DDI-DrugBank.d543.s2.e1" charOffset="43-61"
            type="group" text="antiglaucoma agents"/>
        <pair id="DDI-DrugBank.d543.s2.p0" e1="DDI-DrugBank.d543.s2.e0"
