    <sentence id="DDI-MedLine.d55.s0" text="Increased hepatotoxicity of acetaminophen by concomitant administration of caffeine in the rat.">
        <entity id="DDI-MedLine.d55.s0.e0" charOffset="28-40"
            type="drug" text="acetaminophen"/>
        <entity id="DDI-MedLine.d55.s0.e1" charOffset="75-82"
