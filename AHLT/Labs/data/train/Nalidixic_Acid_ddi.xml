        <pair id="DDI-DrugBank.d427.s8.p2" e1="DDI-DrugBank.d427.s8.e0"
            e2="DDI-DrugBank.d427.s8.e3" ddi="false"/>
        <pair id="DDI-DrugBank.d427.s8.p3" e1="DDI-DrugBank.d427.s8.e1"
            e2="DDI-DrugBank.d427.s8.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d427.s8.p4" e1="DDI-DrugBank.d427.s8.e1"
            e2="DDI-DrugBank.d427.s8.e3" ddi="false"/>
        <pair id="DDI-DrugBank.d427.s8.p5" e1="DDI-DrugBank.d427.s8.e2"
            e2="DDI-DrugBank.d427.s8.e3" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s9" text="sucralfate or divalent or trivalent cations such as iron;">
        <entity id="DDI-DrugBank.d427.s9.e0" charOffset="0-9"
            type="drug" text="sucralfate"/>
        <entity id="DDI-DrugBank.d427.s9.e1" charOffset="52-55"
            type="drug" text="iron"/>
        <pair id="DDI-DrugBank.d427.s9.p0" e1="DDI-DrugBank.d427.s9.e0"
            e2="DDI-DrugBank.d427.s9.e1" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s10" text="multivitamins containing zinc;">
        <entity id="DDI-DrugBank.d427.s10.e0" charOffset="0-12"
            type="group" text="multivitamins"/>
        <entity id="DDI-DrugBank.d427.s10.e1" charOffset="25-28"
            type="drug" text="zinc"/>
        <pair id="DDI-DrugBank.d427.s10.p0"
            e1="DDI-DrugBank.d427.s10.e0" e2="DDI-DrugBank.d427.s10.e1" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s11" text="and Videx , (Didanosine), chewable/buffered tablets or the pediatric powder for oral solution may substantially interfere with the absorption of quinolones, resulting in systemic levels considerably lower than desired.">
        <entity id="DDI-DrugBank.d427.s11.e0" charOffset="4-8"
            type="brand" text="Videx"/>
        <entity id="DDI-DrugBank.d427.s11.e1" charOffset="13-22"
            type="drug" text="Didanosine"/>
        <entity id="DDI-DrugBank.d427.s11.e2" charOffset="145-154"
            type="group" text="quinolones"/>
        <pair id="DDI-DrugBank.d427.s11.p0"
            e1="DDI-DrugBank.d427.s11.e0" e2="DDI-DrugBank.d427.s11.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d427.s11.p1"
            e1="DDI-DrugBank.d427.s11.e0" e2="DDI-DrugBank.d427.s11.e2"
            ddi="true" type="mechanism"/>
        <pair id="DDI-DrugBank.d427.s11.p2"
            e1="DDI-DrugBank.d427.s11.e1" e2="DDI-DrugBank.d427.s11.e2"
            ddi="true" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s12" text="These agents should not be taken within the two hour period before or within the two-hour period after nalidixic acid administration.">
        <entity id="DDI-DrugBank.d427.s12.e0" charOffset="103-116"
            type="drug" text="nalidixic acid"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s13" text="Elevated serum levels of cyclosporine have been reported with the concomitant use of some quinolones and cyclosporine.">
        <entity id="DDI-DrugBank.d427.s13.e0" charOffset="25-36"
            type="drug" text="cyclosporine"/>
        <entity id="DDI-DrugBank.d427.s13.e1" charOffset="90-99"
            type="group" text="quinolones"/>
        <entity id="DDI-DrugBank.d427.s13.e2" charOffset="105-116"
            type="drug" text="cyclosporine"/>
        <pair id="DDI-DrugBank.d427.s13.p0"
            e1="DDI-DrugBank.d427.s13.e0" e2="DDI-DrugBank.d427.s13.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d427.s13.p1"
            e1="DDI-DrugBank.d427.s13.e0" e2="DDI-DrugBank.d427.s13.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d427.s13.p2"
            e1="DDI-DrugBank.d427.s13.e1" e2="DDI-DrugBank.d427.s13.e2"
            ddi="true" type="mechanism"/>
    </sentence>
    <sentence id="DDI-DrugBank.d427.s14" text="Therefore, cyclosporine serum levels should be monitored and appropriate cyclosporine dosage adjustments made when these drugs are used concomitantly.">
        <entity id="DDI-DrugBank.d427.s14.e0" charOffset="11-22"
            type="drug" text="cyclosporine"/>
        <entity id="DDI-DrugBank.d427.s14.e1" charOffset="73-84"
