        <entity id="DDI-DrugBank.d427.s8.e2" charOffset="31-38"
            type="drug" text="aluminum"/>
        <entity id="DDI-DrugBank.d427.s8.e3" charOffset="44-50"
            type="drug" text="calcium"/>
        <pair id="DDI-DrugBank.d427.s8.p0" e1="DDI-DrugBank.d427.s8.e0"
            e2="DDI-DrugBank.d427.s8.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d427.s8.p1" e1="DDI-DrugBank.d427.s8.e0"
