        <pair id="DDI-DrugBank.d305.s1.p2" e1="DDI-DrugBank.d305.s1.e1"
            e2="DDI-DrugBank.d305.s1.e2" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d305.s2" text="Binding to plasma protein is not significantly altered by diazepam, diphenylhydantoin, or phenylbutazone.">
        <entity id="DDI-DrugBank.d305.s2.e0" charOffset="58-65"
            type="drug" text="diazepam"/>
        <entity id="DDI-DrugBank.d305.s2.e1" charOffset="68-84"
            type="drug" text="diphenylhydantoin"/>
        <entity id="DDI-DrugBank.d305.s2.e2" charOffset="90-103"
            type="drug" text="phenylbutazone"/>
        <pair id="DDI-DrugBank.d305.s2.p0" e1="DDI-DrugBank.d305.s2.e0"
