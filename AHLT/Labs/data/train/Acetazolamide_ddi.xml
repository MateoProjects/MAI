        <pair id="DDI-DrugBank.d368.s12.p0"
            e1="DDI-DrugBank.d368.s12.e0" e2="DDI-DrugBank.d368.s12.e1"
            ddi="true" type="mechanism"/>
        <pair id="DDI-DrugBank.d368.s12.p1"
            e1="DDI-DrugBank.d368.s12.e0" e2="DDI-DrugBank.d368.s12.e2" ddi="false"/>
        <pair id="DDI-DrugBank.d368.s12.p2"
            e1="DDI-DrugBank.d368.s12.e1" e2="DDI-DrugBank.d368.s12.e2" ddi="false"/>
    </sentence>
    <sentence id="DDI-DrugBank.d368.s13" text="Acetazolamide and sodium bicarbonate used concurrently increases the risk of renal calculus formation.">
        <entity id="DDI-DrugBank.d368.s13.e0" charOffset="0-12"
            type="drug" text="Acetazolamide"/>
        <entity id="DDI-DrugBank.d368.s13.e1" charOffset="18-35"
