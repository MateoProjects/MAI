    <sentence id="DDI-DrugBank.d223.s9" text="The extent to which SSRI-TCA interactions may pose clinical problems will depend on the degree of inhibition and the pharmacokinetics of the SSRI involved.">
        <entity id="DDI-DrugBank.d223.s9.e0" charOffset="20-23"
            type="group" text="SSRI"/>
        <entity id="DDI-DrugBank.d223.s9.e1" charOffset="25-27"
            type="group" text="TCA"/>
        <entity id="DDI-DrugBank.d223.s9.e2" charOffset="141-144"
            type="group" text="SSRI"/>
        <pair id="DDI-DrugBank.d223.s9.p0" e1="DDI-DrugBank.d223.s9.e0"
            e2="DDI-DrugBank.d223.s9.e1" ddi="true" type="int"/>
        <pair id="DDI-DrugBank.d223.s9.p1" e1="DDI-DrugBank.d223.s9.e0"
