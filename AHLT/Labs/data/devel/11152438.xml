    <sentence id="DDI-MedLine.d47.s1" text="The in vitro anti-cryptosporidial activity of buforin II alone and in combination with azithromycin and minocycline was investigated. ">
        <entity id="DDI-MedLine.d47.s1.e0" charOffset="46-55"
            type="drug_n" text="buforin II"/>
        <entity id="DDI-MedLine.d47.s1.e1" charOffset="87-98"
            type="drug" text="azithromycin"/>
        <entity id="DDI-MedLine.d47.s1.e2" charOffset="104-114"
            type="drug" text="minocycline"/>
        <pair id="DDI-MedLine.d47.s1.p0" e1="DDI-MedLine.d47.s1.e0"
            e2="DDI-MedLine.d47.s1.e1" ddi="false"/>
        <pair id="DDI-MedLine.d47.s1.p1" e1="DDI-MedLine.d47.s1.e0"
