            type="group" text="sympathomimetic amines"/>
        <pair id="DDI-DrugBank.d746.s0.p0" e1="DDI-DrugBank.d746.s0.e0"
            e2="DDI-DrugBank.d746.s0.e1" ddi="false"/>
        <pair id="DDI-DrugBank.d746.s0.p1" e1="DDI-DrugBank.d746.s0.e0"
