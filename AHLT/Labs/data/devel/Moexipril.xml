    <sentence id="DDI-DrugBank.d640.s0" text="Diuretics: Excessive reductions in blood pressure may occur in patients on diuretic therapy when ACE inhibitors are started. ">
        <entity id="DDI-DrugBank.d640.s0.e0" charOffset="0-8"
            type="group" text="Diuretics"/>
